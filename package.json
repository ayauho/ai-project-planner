{
  "name": "ai-project-planner",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "node src/server/server.js",
    "build": "next build",
    "start": "NODE_ENV=production node src/server/server.js",
    "test": "NODE_ENV=test jest",
    "lint": "eslint --ext .ts,.tsx .",
    "lint:fix": "eslint --ext .ts,.tsx . --fix",
    "test:project": "jest --config jest.config.mjs --detectOpenHandles src/tests/lib/project",
    "test:project-creation": "jest src/tests/lib/project/creation/",
    "test:project-selection": "jest src/tests/lib/project/selection/",
    "test:api-config": "jest src/tests/lib/ai/config/",
    "test:request": "jest src/tests/lib/ai/request --detectOpenHandles",
    "test:integration": "jest src/tests/integration --detectOpenHandles",
    "test:real": "node --experimental-vm-modules node_modules/jest/bin/jest.js --config src/tests/integration/ai/real/jest.real.config.mjs",
    "test:task-ops": "jest src/tests/lib/task/operations/*.test.ts",
    "test:task": "jest src/tests/lib/task --verbose",
    "test:hierarchy": "jest src/tests/lib/task/hierarchy/ --detectOpenHandles",
    "test:layout": "jest src/tests/lib/svg/layout/*.test.ts src/tests/lib/svg/layout/**/*.test.ts",
    "test:visual": "jest src/tests/lib/svg/visual/",
    "test:controls": "jest src/tests/lib/svg/controls --coverage",
    "test:side-panel": "jest src/tests/components/workspace/side-panel --passWithNoTests",
    "test:workspace-area": "jest src/tests/components/workspace/area --detectOpenHandles",
    "test:interaction": "jest src/tests/components/workspace/interaction",
    "test:state": "./node_modules/.bin/jest \"src/tests/lib/state/.*\\.test\\.[jt]sx?$\"",
    "test:persistence": "jest src/tests/lib/state/persistence/",
    "test:realtime": "jest src/tests/lib/state/realtime --detectOpenHandles",
    "build:clean": "node build.mjs",
    "dev:clean": "rm -rf .next && npm run dev",
    "test:watch": "NODE_ENV=test jest --watch",
    "test:coverage": "NODE_ENV=test jest --coverage"
  },
